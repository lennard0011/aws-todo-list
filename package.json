{
  "name": "src",
  "version": "0.1.0",
  "bin": {
    "src": "bin/src.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --maxWorkers=1 --silent",
    "format": "prettier --write ./**/*.ts",
    "lint": "eslint src/** && prettier -c src",
    "lint:fix": "eslint --fix src/** && prettier --write src",
    "preall:deploy": "npm run build && npm run webapp:build",
    "all:deploy": "npx cdk deploy --all --concurrency 10 --require-approval never",
    "prebackend:deploy": "npm run build",
    "backend:deploy": "npx cdk deploy 'ToDoListBackend'",
    "preauthentication:deploy": "npm run build",
    "authentication:deploy": "npx cdk deploy 'ToDoListDeployment/Pipeline/Deploy/ToDoListAuthentication'",
    "webapp:dev": "vite",
    "webapp:build": "vite build",
    "webapp:preview": "vite preview",
    "prewebapp:deploy": "npm run build && npm run webapp:build",
    "webapp:deploy": "npx cdk deploy 'ToDoListDeployment/Pipeline/Deploy/ToDoListWebapp'"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@eslint/js": "^9.17.0",
    "@tanstack/router-devtools": "^1.51.5",
    "@tanstack/router-plugin": "^1.51.0",
    "@types/aws-lambda": "^8.10.138",
    "@types/jest": "^29.5.14",
    "@types/node": "20.12.7",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-mocha": "^10.5.0",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^4.0.0-beta1",
    "prettier": "^3.2.5",
    "ts-jest": "^29.2.5",
    "typescript": "~5.4.5",
    "typescript-eslint": "^8.19.1",
    "vite": "^5.2.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.596.0",
    "@aws-sdk/lib-dynamodb": "^3.596.0",
    "@tanstack/react-router": "^1.51.5",
    "aws-amplify": "^6.3.0",
    "aws-cdk-lib": "^2.140.0",
    "constructs": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "src": "file:",
    "ts-node": "^10.9.2"
  }
}
